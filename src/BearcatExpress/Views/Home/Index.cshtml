<div class="container">
  <div class="row">
   <div class="col-sm-9">
       <div class="row"></div>
        <div class="row"></div>
       <div id="myCarousel" class="carousel slide" data-ride="carousel" style="padding-top:100px;">
           <ol class="carousel-indicators">
               <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
               <li data-target="#myCarousel" data-slide-to="1"></li>
               <li data-target="#myCarousel" data-slide-to="2"></li>
               <li data-target="#myCarousel" data-slide-to="3"></li>
           </ol>
           <div class="carousel-inner" role="listbox">
               <div class="item active">
                   <img src="~/images/image1.jpg" alt="Image 1">
               </div>

               <div class="item">
                   <img src="~/images/image2home.jpg" alt="Image 2">
               </div>

               <div class="item">
                   <img src="~/images/image3home.jpg" alt="Image 3">
               </div>

               <div class="item">
                   <img src="~/images/imagehome4.jpg" alt="Image 4">
               </div>
           </div>

           <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
               <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
               <span class="sr-only">Previous</span>
           </a>
           <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
               <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
               <span class="sr-only">Next</span>
           </a>
       </div>
    </div>
    <div class="col-sm-3 ">
        <h3> Price Estimator</h3>
        <form class="form-horizontal" role="form">
            <div class="form-group">
                <label class="control-label" for="Origin">Origin:</label>
                <div>
                    <input type="text" class="form-control" id="Origin" placeholder="Enter Origin">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label" for="Destination">Destination:</label>
                <div>
                    <input type="text" class="form-control" id="Destination" placeholder="Enter Destination">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label" for="Weights">Weight(Kg's)</label>
                <div>
                    <input type="text" class="form-control" id="Weights" placeholder="Enter Destination">
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-offset-3">
                    <button type="submit" class="btn btn-default">Estimate</button>
                </div>
            </div>
        </form>


    </div>
  </div>
</div>